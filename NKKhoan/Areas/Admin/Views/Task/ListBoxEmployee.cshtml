@model NKKhoan.Areas.Admin.ViewModels.EmployeeListBoxViewModel

<div class="form-group">
    @Html.LabelFor(model => model.employeeName, htmlAttributes: new { @class = "control-label col-md-3" })
    <div class="col-md-10">

        @Html.ListBoxFor(model => model.selectedemployee, Model.employees, new { @class = "form-control selectpicker", data_live_search = "true" })
        <a href="~/Admin/Employee/Create" class="btn btn-primary">
            Thêm mới
        </a>
    </div>
</div>